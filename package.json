    "serve": "vite preview"
  },
  "dependencies": {
    "dayjs": "^1.10.6",
    "element-plus": "1.0.2-beta.54",
    "lodash": "^4.17.21",
    "vue": "3.1.4"
  },
  "devDependencies": {
    "@rollup/pluginutils": "^4.1.0",
    "@types/lodash": "^4.14.171",
    "@types/node": "^16.0.1",
    "@vitejs/plugin-vue": "^1.2.4",
    "@vue/compiler-sfc": "^3.1.4",
    "rollup-plugin-typescript2": "^0.30.0",
    "sass": "^1.35.2",
    "sass-loader": "^12.1.0",
    "ts-morph": "^11.0.0",
    "typedoc": "^0.21.2",
    "typescript": "^4.3.5",
    "vite": "^2.4.1",
    "vite-plugin-dts": "^0.5.2",
