  "license": "MIT",
  "keywords": [
    "elp-crud",
    "crud",
    "element-plus",
    "vite",
    "vue",
    "vue3",
    "typescript",
    "ts"
  ],
  "files": [
    "lib"
  ],
  "main": "./lib/index.umd.js",
  "module": "./lib/index.es.js",
